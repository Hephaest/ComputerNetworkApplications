            }
          },
          {
            breakpoint: 600,
            settings: {
              slidesToShow: 1,
              centerMode: false
            }
          }
        ]
      });
      $(slickId).on('afterChange', function(slick, currentSlide) {
        var slides = document.querySelectorAll(slickId + ' .slick-slide');
        var toggleClass = 'unavailable';

        if (slides[0].getAttribute('aria-hidden') === 'false') {
          document.querySelector(parentId + ' .carousel-arrow.left-arrow').classList.add(toggleClass);
        }
        else {
          document.querySelector(parentId + ' .carousel-arrow.left-arrow').classList.remove(toggleClass);
        }

        if (slides[slides.length - 1].getAttribute('aria-hidden') === 'false') {
          document.querySelector(parentId + ' .carousel-arrow.right-arrow').classList.add(toggleClass);
        }
        else {
          document.querySelector(parentId + ' .carousel-arrow.right-arrow').classList.remove(toggleClass);
        }
      });

      // make the cards the same height
      var equalizeCards = function(selector) {
        var resetCards = function() {
          var cards = document.querySelectorAll(selector + ' .card-wrapper .card-content');
          Array.prototype.forEach.call(cards, function(card) {
            card.style.height = 'auto';
          });
        };
        var setCardSize = function() {
          var maxHeight = 0;
          var cards = document.querySelectorAll(selector + ' .card-wrapper .card-content');
          Array.prototype.forEach.call(cards, function(card) {
            var height = card.getBoundingClientRect().height;
            if (height > maxHeight) {
              maxHeight = Math.ceil(height);
            }
          });
          Array.prototype.forEach.call(cards, function(card) {
            card.style.height = maxHeight.toString() + 'px';
          });
        };

        window.addEventListener('resize', function(e) {
          setTimeout(function() {
            resetCards();
            setCardSize();
          }, 200);
        });

        return {
          init: function() {
            setCardSize();
          }
        };
      };

      equalizeCards(slickId).init();

      $('.article-card img').unveil();
    });
    </script>
